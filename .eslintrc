{
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true
  },
  "extends": "defaults",
  "globals": {
  },
  "parser": "babel-eslint",
  "plugins": [
    "eslint-plugin-jasmine"
  ],
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "keyword-spacing": "error",
    "no-console": 0,
    "no-const-assign": "error",
    "no-spaced-func": "error",
    "no-var": 2,
    "no-trailing-spaces": "error",
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "comma-dangle": 0,
    "prefer-const": "error",
    "semi": [
      "error",
      "always"
    ],
    "space-before-blocks": "error",
    "space-infix-ops": "error"
  }
}
